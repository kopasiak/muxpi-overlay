ARCH=arm
GOARCH="${ARCH}"
GOARM=7
FEATURES="buildpkg binpkg-multi-instance noman noinfo nodoc"

PYTHON_TARGETS="python2_7 -python3_5 python3_6"
PYTHON_SINGLE_TARGET="-python3_5 python3_6"
