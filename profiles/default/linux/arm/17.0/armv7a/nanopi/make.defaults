USE="minimal -sendmail -pam -acl -seccomp -xattr -vtv -native-extensions -ipv6 -nls -ncurses -mime"
CFLAGS="-O2 -pipe -fomit-frame-pointer -march=armv7-a -mtune=cortex-a7 -mfloat-abi=hard -mfpu=neon-vfpv4"
